/* pages/checkAgain/checkAgain.wxss */
.textBox{
  width: 100%;
  padding: 30rpx 0;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.textBox text{
  line-height: 80rpx;
}
.title{
  font-size: 50rpx;
}
.btnBox{
  width: 100%;
  display: flex;
  justify-content: center;
}
.btnBox .btn{
  background: #f10215;
  width: 60%;
  font-size: 40rpx;
  color: #FFFFFF;
}